<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Table of User Profiles"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <navtitle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Title"
                        name="./selectuser"
                        required="false"/>
                    <multilinks
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                        composite="{Boolean}true"
                        fieldLabel="TableComponent">
                        <field
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/container"
                            name="./userdetails">
                            <items jcr:primaryType="nt:unstructured">
                                <column
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                    <items jcr:primaryType="nt:unstructured">
                                        <labelfortable
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                            fieldDescription="Description of Manage Profile User"
                                            fieldLabel="Enter Label For Table Border"
                                            name="./enterlabelfortableborder"/>
                                    </items>
                                </column>
                            </items>
                        </field>
                    </multilinks>
                    <requireCheckboxes
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/select"
                        class="dropdownselect"
                        defaultValue="requireCheckbox"
                        fieldLabel="Require Checkbox?"
                        name="./requireCheckboxes"
                        value="requireCheckbox">
                        <items jcr:primaryType="nt:unstructured">
                            <yes
                                jcr:primaryType="nt:unstructured"
                                text="Checkbox is required"
                                value="requireCheckbox"/>
                            <no
                                jcr:primaryType="nt:unstructured"
                                text="Checkbox is not required"
                                value="notRequireCheckbox"/>
                        </items>
                    </requireCheckboxes>
                    <labelforcustocolumns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Description of Customised Columns"
                        fieldLabel="Enter Label For Customize Columns"
                        name="./enterlabelforcustomizecolumns"/>
                    <buttonforapply
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Description of Customised Columns"
                        fieldLabel="Text For Apply Button"
                        name="./textforapplybutton"/>
                    <continuebutton
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldLabel="Continue Button"
                        name="./continue"
                        required="false"/>
                    <continuebuttonurl
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                        fieldLabel="continue Button Link"
                        name="./continuelink"
                        rootPath="/content/vodafonemvp/en"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
